#include<bits/stdc++.h>
#include <iostream>
#include <climits>
using namespace std;

int main() {
    int n;
    cin>>n;
    
    int arr[n];
    
    
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   int k;
   cin>>k;
   
   priority_queue<int,vector<int>,greater<int>>p;  // using minheap we can find the kth smallest element and similarly using maxheap we can find kth largest element
   
   for(int i=0;i<n;i++){
       p.push(arr[i]);
   }
   int ans=1;
   int i=1;
   while(!p.empty()){
       if(i==k){
           ans=p.top();
           break;
       }
       i++;
       p.pop();
   }
   cout<<ans<<endl;
    
    return 0;
}
