
int main() {
     
    int arr[9]={12, 11, -13, -5, 6, -7, 5, -3, -6};  // output-{-13,, -5, -7, -3, -6, 12, 5, 11, 6 }
    
    int i=0;
    int j=0;
    while(i<9){
        if(arr[i]<0){
            swap(arr[i],arr[j]);
            j++;
        }
        i++;
    }
    for(int i=0;i<9;i++){
        cout<<arr[i]<<" ";
    }
    
    
   
    return 0;
}
