#include <iostream>
#include <climits>
using namespace std;

int maximum(int *arr,int n){
    int maxi=INT_MIN;
    for(int i=0;i<n;i++){
        maxi=max(maxi,arr[i]);
    }
    return maxi;
}
int minimum(int *arr,int n){
    int mini=INT_MAX;
    for(int i=0;i<n;i++){
        mini=min(mini,arr[i]);
    }
    return mini;
}


int main() {
    
   int arr[6]={1,2,3,4,5,6};
   cout<<maximum(arr,6)<<endl;
   cout<<minimum(arr,6)<<endl;
    
    
    return 0;
}
